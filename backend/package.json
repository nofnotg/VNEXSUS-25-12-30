{
  "name": "medreport-ai-backend",
  "version": "1.0.0",
  "description": "Medical Report AI Backend Server",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:stream": "jest --testPathPattern=\"streamProcessingOptimizer|largeFileHandler|fileProcessingService\"",
    "test:vision": "node test-vision.js",
    "check:env": "node -e \"console.log('Backend Environment:', {NODE_ENV: process.env.NODE_ENV, PORT: process.env.PORT, VISION_ENABLED: process.env.ENABLE_VISION_OCR})\""
  },
  "dependencies": {
    "@google-cloud/pubsub": "^5.1.0",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/vision": "^5.3.2",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.11.0",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "sharp": "^0.34.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0"
  }
}
