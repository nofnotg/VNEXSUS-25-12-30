{
  "name": "medreport-ai-mvp",
  "version": "1.0.0",
  "type": "module",
  "description": "Medical Report AI Analysis System",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node backend/app.js",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "serve:report": "node scripts/serve-report.js",
    "analyze:coord": "node backend/tools/offlineCoordAnalyzer.js",
    "dev:report": "concurrently \"npm run analyze:coord\" \"npm run serve:report\"",
    "validate:coords": "node backend/tools/batchReprocessCases.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:progressive-rag": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPatterns=progressiveRAG",
    "test:integration": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPatterns=integration",
    "test:rag": "node test-progressive-rag.js",
    "test:rag:integration": "node -e \"import('./src/rag/index.js').then(m => m.ProgressiveRAGManager).then(async M => { const mgr = new M({enableTesting: true}); await mgr.initialize(); const results = await mgr.getTestRunner().runIntegrationTestsOnly(); console.log('Integration tests completed:', results); await mgr.stop(); })\"",
    "test:rag:performance": "node -e \"import('./src/rag/index.js').then(m => m.ProgressiveRAGManager).then(async M => { const mgr = new M({enableTesting: true}); await mgr.initialize(); const results = await mgr.getTestRunner().runPerformanceTestsOnly(); console.log('Performance tests completed:', results); await mgr.stop(); })\"",
    "optimize:rag": "node -e \"import('./src/rag/index.js').then(m => m.ProgressiveRAGManager).then(async M => { const mgr = new M({enableOptimization: true}); await mgr.initialize(); await mgr.start(); const results = await mgr.runOptimization(); console.log('Optimization completed:', results); await mgr.stop(); })\"",
    "test:episodes:smoke": "node scripts/outpatient-episodes-smoke.js",
    "test:episodes:smoke:min": "node scripts/outpatient-episodes-smoke-min.js",
    "analyze:episodes:case": "node scripts/run-outpatient-episodes-case-sample.js",
    "analyze:episodes:compare": "node scripts/compare-outpatient-episodes-case.js",
    "report:csv": "node src/scripts/generateComparisonCsv.js --input results/outpatient-episodes-case-comparison.json --output reports/outpatient-episodes-case-comparison.csv",
    "report:html": "node src/scripts/cliGenerateComparisonHtml.js --input results/outpatient-episodes-case-comparison.json --output reports/outpatient-episodes-case-comparison.html --title \"Outpatient Episodes Comparison\"",
    "report:all": "npm run report:csv && npm run report:html",
    "report:summary": "node src/scripts/cliGenerateSummary.js --input results/outpatient-episodes-case-comparison.json --output reports/outpatient-episodes-summary.json"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.7.2",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.7",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "express-rate-limit": "^8.2.1",
    "iconv-lite": "^0.6.3",
    "idb": "^8.0.3",
    "multer": "^2.0.2",
    "natural": "^6.12.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.67.3",
    "playwright": "^1.48.2",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^30.2.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.48.1"
  }
}
